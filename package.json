{
  "name": "vitae-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "server": "tsx watch server/index.ts",
    "server:prod": "NODE_ENV=production tsx server/index.ts",
    "start": "npm run server:prod",
    "dev:all": "concurrently \"npm run dev\" \"npm run server\"",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:reset": "docker compose down -v && docker compose up -d",
    "db:schema": "docker exec -i vitae_mysql_dev mysql -uapp_user -papp_password consultoria_edu < database/init/01-schema.sql",
    "db:seed": "docker exec -i vitae_mysql_dev mysql -uapp_user -papp_password consultoria_edu < database/init/02-seed.sql",
    "deploy": "bash scripts/deploy.sh"
  },
  "dependencies": {
